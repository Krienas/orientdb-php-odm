{
    "name": "doctrine/orientdb-odm",
    "type": "library",
    "description": "A set of PHP libraries in order to use OrientDB from PHP.",
    "keywords": ["orientdb"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alessandro Nadalin",
            "email": "alessandro.nadalin@gmail.com",
            "homepage": "http://www.odino.org"
        },
        {
            "name": "David Funaro",
            "email": "ing.davidino@gmail.com",
            "homepage": "http://davidfunaro.com"
        },
        {
            "name": "Daniele Alessandri",
            "email": "suppakilla@gmail.com",
            "homepage": "http://clorophilla.net"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/stuartcarnie/athletic.git"
        }
    ],
    "autoload": {
        "psr-0": {
            "Doctrine\\OrientDB": "src/",
            "Doctrine\\ODM\\OrientDB": "src/",
            "Benchmarks": "benchmarks/"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "doctrine/common": "~2.2",
        "symfony/finder": "~2.3",
        "symfony/class-loader": "~2.3",
        "guzzle/guzzle": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*"
        ,"athletic/athletic": "2.x"
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable"
}
